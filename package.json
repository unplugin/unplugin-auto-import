{
  "name": "unplugin-auto-import",
  "type": "module",
  "version": "20.3.0",
  "packageManager": "pnpm@10.28.0",
  "description": "Register global imports on demand for Vite and Webpack",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/unplugin/unplugin-auto-import#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unplugin/unplugin-auto-import.git"
  },
  "bugs": {
    "url": "https://github.com/unplugin/unplugin-auto-import/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "astro",
    "webpack",
    "rolldown",
    "rollup",
    "rspack",
    "auto-import",
    "transform"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./astro": "./dist/astro.mjs",
    "./esbuild": "./dist/esbuild.mjs",
    "./nuxt": "./dist/nuxt.mjs",
    "./rolldown": "./dist/rolldown.mjs",
    "./rollup": "./dist/rollup.mjs",
    "./rspack": "./dist/rspack.mjs",
    "./types": "./dist/types.mjs",
    "./vite": "./dist/vite.mjs",
    "./webpack": "./dist/webpack.mjs",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.cts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown -w",
    "lint": "eslint .",
    "lint:fix": "nr lint --fix",
    "typecheck": "tsc",
    "play": "pnpm -C playground run dev",
    "prepublishOnly": "nr build",
    "release": "bumpp",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "peerDependencies": {
    "@nuxt/kit": "^4.0.0",
    "@vueuse/core": "*"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@vueuse/core": {
      "optional": true
    }
  },
  "dependencies": {
    "local-pkg": "^1.1.2",
    "magic-string": "^0.30.21",
    "picomatch": "^4.0.3",
    "unimport": "^5.6.0",
    "unplugin": "^2.3.11",
    "unplugin-utils": "^0.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.0.0",
    "@antfu/ni": "^28.2.0",
    "@antfu/utils": "^9.3.0",
    "@nuxt/kit": "^4.2.2",
    "@nuxt/schema": "^4.2.2",
    "@svgr/plugin-jsx": "^8.1.0",
    "@types/node": "^25.0.8",
    "@types/picomatch": "^4.0.2",
    "@types/resolve": "^1.20.6",
    "@vueuse/metadata": "^14.1.0",
    "bumpp": "^10.4.0",
    "eslint": "^9.39.2",
    "fast-glob": "^3.3.3",
    "publint": "^0.3.16",
    "rolldown": "^1.0.0-beta.60",
    "rollup": "^4.55.1",
    "tsdown": "^0.20.0-beta.3",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.17",
    "webpack": "^5.104.1"
  }
}
